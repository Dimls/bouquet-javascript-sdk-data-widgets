{{#if results}}

	{{#with results}}
	<table class='sq-table'>
	    <thead>
	        <tr>
	            {{#cols}}<th>{{lname}}</th>{{/cols}}
	        </tr>
	    </thead>
	    <tbody>
	        {{#rows}}
	           <tr>
	               {{#v}}
	                  <td>{{this}}</td>
	               {{/v}}
	           </tr>
	        {{/rows}}
	    </tbody>
	</table>
	{{/with}}
{{else}}

	{{#if done}}
  		No Data
  	{{else}}

	<div class='sq-loading' style='position:absolute; width:100%; top:40%; z-index: 1;'>
		<div class="spinner">
		<div class="rect5"></div>
		<div class="rect4"></div>
		<div class="rect3"></div>
		<div class="rect2"></div>
		<div class="rect1"></div>
		<div class="rect2"></div>
		<div class="rect3"></div>
		<div class="rect4"></div>
		<div class="rect5"></div>
		</div>
	</div>

  	{{/if}}
{{/if}}
